<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs') %>
<body>
<%- include('./partials/nav.ejs') %>
<h1>Contacts:</h1>
<% const name = 'Jack (ejs)' %>
<ul>
    <% if (contacts.length) { %>
        <% contacts.forEach(({ link, name }) => { %>
            <li>
                <a href="<%= link %>"><%= name %></a>
            </li>
        <% }) %>
    <% } %>
    <li><%= name %></li>
</ul>
</body>
</html>
